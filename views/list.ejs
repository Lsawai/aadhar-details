<%- include ("header") -%>

<body>
    <h1> List of All Aadhar Card Details</h1>
    <div>
        <table class="table table-striped table-bordered">
            <thead class="thead-dark ">
                <tr>
                    <th scope="col">#.</th> 
                    <th scope="col">Aadhar No.</th> 
                    <th scope="col">Name</th>
                    <th scope="col"> Age</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i<Aadharitems.length; i++) {%>
                    <tr>
                        <th scope="row"><%=i+1%></th>
                        <td><%=Aadharitems[i].Aadhar_id %></td> 
                        <td><%= Aadharitems[i].Name%></td>
                        <td><%= Aadharitems[i].Age%></td>
                    </tr>          
                <% } %>
            </tbody>
            
        </table>
    </div>
</body>

<%- include ("footer") -%>